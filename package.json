{
	"name": "root-docs",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"build": "next build",
		"dev": "next dev",
		"algolia": "node ./.github/algoliaSync.mjs",
		"tsc": "tsc --noEmit",
		"format": "prettier --check '**/*.ts' '**/*.json' '**/*.yml' '**/*.mjs' 'content/**/*.mdx' --config .prettierrc.json --log-level warn",
		"format:fix": "prettier --write '**/*.ts' '**/*.json' '**/*.yml' '**/*.mjs' 'content/**/*.mdx' --config .prettierrc.json --log-level warn",
		"spell": "cspell --config .cspell.json --quiet --gitignore '*' 'content/**/*.mdx' 'app/**/*' 'components/**/*'"
	},
	"dependencies": {
		"algoliasearch": "^4.22.1",
		"fumadocs-core": "8.2.0",
		"fumadocs-mdx": "8.0.3",
		"fumadocs-ui": "8.2.0",
		"lucide-react": "^0.331.0",
		"next": "14.1.0",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"zod": "3.22.4"
	},
	"devDependencies": {
		"@types/mdx": "2.0.11",
		"@types/node": "20.11.19",
		"@types/react": "18.2.55",
		"@types/react-dom": "18.2.19",
		"autoprefixer": "^10.4.17",
		"cspell": "^8.3.2",
		"eslint-plugin-mdx": "^3.1.5",
		"postcss": "^8.4.35",
		"prettier": "^3.2.5",
		"tailwindcss": "^3.4.1",
		"typescript": "5.3.3"
	}
}